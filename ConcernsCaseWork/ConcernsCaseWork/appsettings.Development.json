{
	"Sentry": {
		"Debug": true
	},
	"Logging": {
		"LogLevel": {
			"Default": "Information",
			"Microsoft": "Warning",
			"Microsoft.Hosting.Lifetime": "Information"
		}
	},
	"AllowedHosts": "*",
	"Cache": {
		"TimeToLive": "24"
	},
	"TramsApi": {
		"TrustsLimitByPage": "10"
	},
	"Kestrel": {
		"Endpoints": {
			"Https": {
				"Url": "https://*:8001"
			},
			"Http": {
				"Url": "http://*:8000"
			}
		},
		"Certificates": {
			"Development": {
				"Password": ""
			}
		}
	}
}
