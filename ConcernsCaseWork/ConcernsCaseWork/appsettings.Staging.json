{
	"Sentry": {
		"Debug": true
	},
	"Logging": {
		"LogLevel": {
			"Default": "Information",
			"Microsoft": "Warning",
			"Microsoft.Hosting.Lifetime": "Information"
		}
	},
	"AllowedHosts": "*",
	"Cache": {
		"TimeToLive": "24"
	},
	"TramsApi": {
		"TrustsLimitByPage": "1",
		"TrustsPerPage": "100",
		"MilliSecondPauseBeforeSeach": "3000",
		"MinCharsRequiredToSeach": "3"
	}
}
