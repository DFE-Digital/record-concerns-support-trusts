{
  "name": "concernscasework.cypresstests",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "cy:open": "cypress open --browser edge",
    "cy:run": "npm run clean:reports && npm run cy:run:only",
    "cy:run:only": "cypress run --browser edge",
    "cy:ci": "npm run clean:reports && npm run cy:parallel",
    "cy:parallel": "cypress-parallel -s cy:run:only -t 2 -d './cypress/e2e' -r mochawesome -o 'reportDir=./cypress/reports/mocha,overwrite=false,html=false'",
    "cy:notify": "cypress-slack-reporter",
    "clean:reports": "rm -rf cypress/reports/mocha/*.json",
    "combine:reports": "mochawesome-merge  cypress/reports/mocha/*.json> mochareports/report.json",
    "create:html:report": "marge  mochareports/report.json -f report -o mochareports",
    "generate:html:report": "npm run combine:reports && npm run create:html:report"
  },
  "keywords": [
    "e2e",
    "regression",
    "automation"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "axe-core": "^4.6.1",
    "cypress": "^12.7.0",
    "cypress-axe": "^1.4.0",
    "cypress-localstorage-commands": "^1.6.1",
    "cypress-multi-reporters": "^1.5.0",
    "cypress-parallel": "^0.12.0",
    "cypress-slack-reporter": "^1.5.3",
    "mocha": "^9.2.2",
    "mochawesome": "^7.0.1",
    "mochawesome-merge": "^4.2.1",
    "mochawesome-report-generator": "^6.0.1",
    "puppeteer": "^19.1.0",
    "typescript": "^4.8.4"
  }
}
